<header>
  <div align="center">
    <h1>Submit a New Post</h1>
  </div>
</header>

<ul class="errors">
  <% @vee_post.errors.full_messages.each do |message| %>
    <li><%= message %></li>
  <% end %>
</ul>

<form action="/vee_posts" method="POST">
  <div class="form-group">
    <label for="photo_url">Photo URL</label>
    <input type="text" id="photo_url" name="photo_url">
  </div>
  <div class="form-group">
    <button class="button" type="submit">Post</button>
  </div>
</form>


    
